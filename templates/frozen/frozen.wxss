.frozenWrap{
    width:100%;
    height:100%;
    position:fixed;
    top:0rpx;
    left:0rpx;
    background:rgba(0,0,0,0.3);
    z-index:10000;
    display:flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}
.frozenContent{
    width:90%;
    height:180rpx;
    background:#fff;
    border-radius:10rpx;
}
.frozenTitle{
    width:100%;
    height:60rpx;
    text-align:center;
    line-height:60rpx;
    font-size:32rpx;
    color:#7b7b7b;
}
.frozenText{
    width:100%;
    height:120rpx;
    text-align:center;
    line-height:120rpx;
    font-size:34rpx;
}