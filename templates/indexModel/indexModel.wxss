/* 瑜伽 课外快乐 驾考 */
.indexBackground {
    width: 750rpx;
    min-height:650rpx;
    background:#F7A784;
}
.empty{
    width:750rpx;
    height:25rpx;
}
.indexOptons{
    width:700rpx;
    height:350rpx;
    background:#fff;
    border-radius:5px;
    margin:0rpx auto 25rpx;
}
.indexOptonsChild{
    width:175rpx;
    height:175rpx;
    float:left;
}
.indexOptionsTop{
    width:100rpx;
    height:100rpx;
    margin:20rpx auto 0rpx;
    background:#0ff;
}
.indexOptionsTop image{
    width:100rpx;
    height:100rpx;
}
.indexOptionsBottom{
    width:100%;
    line-height:40rpx;
    font-size:30rpx;
    text-align:center;
    margin-top:10rpx;
}
 /* 课程导航 */
.indexCourses{
    width:700rpx;
    min-height:300rpx;
    background:#fff;
    border-radius:5px;
    margin:0rpx auto;
    padding-bottom:25rpx;
}
.indexCoursesTitle{
    width:700rpx;
    line-height:80rpx;
    border-radius:5px 5px 0px 0px;
    color:#F75B69;
    font-size:40rpx;
    text-align:center;
}
.indexCoursesContent{
    width:700rpx;
    min-height:220rpx;
    margin-bottom:20rpx;
    border:1rpx solid #efefef;
}
.leftImage{
    width:280rpx;
    height:180rpx;
    float:left;
    margin-top:20rpx;
    margin-left:20rpx;
}
.leftImage image{
    width:280rpx;
    height:180rpx;
}
.rightText{
    width:360rpx;
    height:180rpx;
    float:left;
    margin-top:20rpx;
    margin-left:20rpx;
}
.rightTextTitle{
    width:360rpx;
    line-height:80rpx;
    font-size:35rpx;
}
.rightTextPrice{
    width:360rpx;
    height:40rpx;
    font-size:26rpx;
    margin-top:20rpx;
}
.rightTextPrice text:nth-child(1){
    font-size:28rpx;
    color:#F82335;
}
.rightTextPrice text:nth-child(2){
    font-size:22rpx;
    color:#7b7b7b;
    text-decoration:line-through;
}
/* 幼儿园 */
.twoWrap{
    width: 750rpx;
    min-height:650rpx;
    background:#f2f2f2;
    padding-bottom:20rpx;
}
.twoNav{
    width:750rpx;
    height:400rpx;
    background:#fff;
}
.twoNavChild{
    width:180rpx;
    height:200rpx;
    background:lightblue;
    float:left;
    margin-left:7.5rpx;
}
.twoNavChildTop{
    width:120rpx;
    height:120rpx;
    margin-top:10rpx;
    margin-left:30rpx;
}
.twoNavChildTop image{
    width:120rpx;
    height:120rpx;
}
.twoNavChildBottom{
    width:180rpx;
    height:50rpx;
    font-size:30rpx;
    text-align:center;
    margin-top:10rpx;
}
.twoTitle{
    width:750rpx;
    height:100rpx;
    line-height:100rpx;
    text-align:center;
    font-size:35rpx;
    color:#E27131;
    font-weight:bold;
}
.twoSwiper{
    width:710rpx;
    height:380rpx;
    background:#fff;
    margin:0rpx auto 0rpx;
}
.twoSwiper swiper{
    background:red;
    border-radius:10rpx;
    background:#f2f2f2;
    height:380rpx;
}
.swiperWrpa{
    width:710rpx;
    height:380rpx;
    background:#fff;
    border-radius:10rpx;
}
.swiperTop{
    width:710rpx;
    height:60rpx;
    line-height:60rpx;
    font-size:30rpx;
    font-weight:bold;
    text-align:center;
}
.swiperBottom{
    width:670rpx;
    height:300rpx;
    background:lightblue;
    margin:0 auto;
}
.twoInfo{
    width:710rpx;
    min-height:500rpx;
    background:#fff;
    margin:0 auto;
    border-radius:10px;
}
.twoInfoSwiper{
    width:710rpx;
    height:320rpx;
}
.twoInfoImage{
    width:670rpx;
    height:280rpx;
    margin:20rpx auto;
    background:lightgreen;
}
.twoInfoContent{
    width:670rpx;
    min-height:460rpx;
    margin:0rpx auto 20rpx;
    font-size:30rpx;
    color:#7b7b7b;
}
/* 美术 */
.threeWrap{
    width:750rpx;
    height:750rpx;
    margin-top:20rpx;
    padding-bottom:20rpx;
}
.threeChild{
    width:250rpx;
    height:250rpx;
    float:left;
}
.threeChildTop{
    width:150rpx;
    height:150rpx;
    margin:20rpx auto 0rpx;
}
.threeChildTop image{
    width:150rpx;
    height:150rpx;
}
.threeChildBottom{
    width:100%;
    height:60rpx;
    font-size:30rpx;
    text-align:center;
    color:#fff;
    margin-top:20rpx;
}
/* 课外活动 */
.fourWrap{
    width:750rpx;
    min-height:500rpx;
    background:lightblue;
}
.fourLesson{
    width:750rpx;
    min-height:330rpx;
}
.fourLessonContent{
    width:320rpx;
    min-height:330rpx;
    background:lightgreen;
    border-radius:10rpx;
    margin-left:30rpx;
    margin-bottom:20rpx;
    display:inline-block;
}
.fourLessonContentTop{
    width:320rpx;
    height:280rpx;
    overflow:hidden;
}
.fourLessonContentTop image{
    width:320rpx;
    height:280rpx;
}
.fourLessonContentBottom{
    width:320rpx;
    height:40rpx;
    font-size:30rpx;
    text-align:center;
}