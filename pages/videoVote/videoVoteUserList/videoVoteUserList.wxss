page{
    background:#F4F4F4;
}
.voteUserWrap{
    width:730rpx;
    height:410rpx;
    margin-left:20rpx;  
    border-radius:10px 0px 0px 0px;
    background:#fff;
    overflow:hidden;
}
.voteUserContent{
    width:730rpx;
    height:330rpx;
    background:#fff;
    position:relative;
    overflow:hidden;
}
.voteUserContent image{
    width:730rpx;
    height:330rpx;
}
.contentLeft{
    height:50rpx;
    line-height:50rpx;
    padding:0px 8rpx;
    position:absolute;
    left:0rpx;
    top:0rpx;
    background:rgba(0,0,0,0.3);
    color:rgba(255,255,255,0.5);
    font-size:26rpx;
}
.contentRight{
    height:50rpx;
    line-height:50rpx;
    font-size:26rpx;
    background:#6D5DDA;
    color:rgba(255,255,255,0.5);
    position:absolute;
    right:0rpx;
    top:0rpx;
    text-align:center;
    padding:0rpx 8rpx;
}
.contentbottom{
    width:730rpx;
    height:60rpx;
    line-height:60rpx;
    background:rgba(0,0,0,0.3);
    color:rgba(255,255,255,0.7);
    font-size:28rpx;
    position:absolute;
    right:0rpx;
    bottom:0rpx;
    text-align:center;
}
.voteUserTitle{
    width:100%;
    height:80rpx;
    line-height:80rpx;
    text-align:center;
    font-size:30rpx;
    font-weight:bold;
    overflow: hidden;
    white-space: nowrap; 
    text-overflow:ellipsis;
}
view.voteUserWrap:nth-child(1){
    margin-top:20rpx;
}