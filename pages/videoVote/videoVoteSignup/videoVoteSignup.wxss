page{
    background:#F7F3F2;
    padding-bottom:20rpx;
}
swiper{
    width:750rpx;
    height:330rpx;
}
.voteInfoImage{
    width:750rpx;
    height:330rpx;
    background:#fff;
}
.voteInfoImage image{
    width:750rpx;
    height:330rpx;
}
.voteInfoStatus{
    width:720rpx;
    height:120rpx;
    background:#5EC8DB;
    margin:40rpx auto 20rpx;
    border-radius:10px;
    overflow:hidden;
}
.voteInfoStatusChild{
    width:238rpx;
    height:90rpx;
    float:left;
    border-right:2rpx solid #fff;
    margin-top:15rpx;
}
view.voteInfoStatusChild:nth-child(3){
    border-right:0rpx;
}
.voteInfoStatusChild view{
    width:238rpx;
    height:45rpx;
    text-align:center;
    font-size:24rpx;
    color:#fff;
}
/* 报名选项 */
.voteSignupInput{
    width:710rpx;
    min-height:100rpx;
    margin:50rpx auto 0rpx;
    background:#fff;
    box-shadow: 2px 2px 2px 2px #70D0DE,-2px -2px 2px 2px #70D0DE;
    border-radius:5px;
    padding-bottom:20rpx;
}
.signUpInputTitle{
    width:100%;
    line-height:60rpx;
    font-size:40rpx;
    text-align:center;
}
.signUpInputName{
    width:630rpx;
    height:80rpx;
    margin:30rpx auto;
}
.signUpInputNameLeft{
    width:140rpx;
    height:70rpx;
    line-height:70rpx;
    font-size:30rpx;
    text-align:center;
    float:left;
}
.signUpInputNameRight{
    width:440rpx;
    height:70rpx;
    float:left;
    margin-left:40rpx;
    border:1px solid #2C7C66;
    border-radius:5px 5px;
}
.signUpInputNameRight input{
    width:425rpx;
    height:60rpx;
    margin-top:5rpx;
    margin-left:15rpx;
    font-size:30rpx;
}

/* 作品封面 */
.signUpInputImage{
    width:630rpx;
    height:280rpx;
    margin:30rpx auto 30rpx;
}
.chooseImageWrap{
    width:440rpx;
    height:280rpx;
    float:left;
    margin-left:40rpx;
}
.chooseImage,.chooseImageO{
    width:210rpx;
    height:270rpx;
    border:1px solid #2C7C66;
    float:left;
    margin-top:3rpx;
    border-radius:5px;
    line-height:270rpx;
    font-size:24rpx;
    color:#7b7b7b;
    text-align:center;
}
.chooseImageO{
    margin-left:12rpx;
    overflow:hidden;
}
.chooseImageO image,.chooseImageO video{
    width:210rpx;
    height:270rpx;
}
.textAreaWrap{
    width:440rpx;
    height:280rpx;
    float:left;
    margin-left:40rpx;
    border:1rpx solid #2C7C66;
    border-radius:5px;
}
.textAreaWrap textarea{
    width:420rpx;
    height:260rpx;
    margin-top:10rpx;
    margin-left:10rpx;
    font-size:30rpx;
}
.signUpBtn{
    width:300rpx;
    height:80rpx;
    color:#fff;
    background:#F67685;
    font-size:30rpx;
    line-height:80rpx;
}

/* 活动介绍 */
.activeInfoWrap{
    width:720rpx;
    min-height:100rpx;
    background:transparent;
    margin:20rpx auto 0rpx;
}
/* 活动介绍  活动说明 */
.activeInfoInfo{
    width:720rpx;
    height:410rpx;
    background:#fff;
    position:relative;
}
.activeInfoInfoTitle{
    width:170rpx;
    height:60rpx;
    background:#0577AB;
    color:#fff;
    border-radius:0px 0px 5px 5px;
    position:absolute;
    top:0rpx;
    left:275rpx;
    font-size:26rpx;
    text-align:center;
    line-height:60rpx;
}
.activeInfoInfo>button{
    width:300rpx;
    height:60rpx;
    line-height:60rpx;
    color:#fff;
    background:#CC2200;
    font-size:28rpx;
    margin-top:40rpx;
}
.activeInfoInfoTime{
    width:680rpx;
    height:100rpx;
    margin:0rpx auto 30rpx;
}
.activeInfoInfoTime view{
    width:680rpx;
    height:50rpx;
    line-height:80rpx;
    font-size:30rpx;
}
.activeInfoInfoTime view:nth-child(2){
    text-align:center;
}
.empty{
    width:100%;
    height:45rpx;
    background:transparent;
}
/* 活动介绍  分享活动 */
.activeInfoShare{
    width:720rpx;
    height:610rpx;
    background:#fff;
    position:relative;
    margin-top:20rpx; 
    overflow:hidden;
}
.activeInfoShareImage{
    width:400rpx;
    height:400rpx;
    margin:90rpx auto 0rpx;
    background:lightgreen;
}
.activeInfoShareImage image{
    width:400rpx;
    height:400rpx;
}
.activeInfoShare button{
    width:400rpx;
    height:60rpx;
    line-height:60rpx;
    font-size:28rpx;
    color:#fff;
    background:#CC2200;
    margin-top:30rpx;
}
/* 活动介绍 活动详情 */
.activeInfoImages{
    width:720rpx;
    min-height:200rpx;
    background:#fff;
    margin-top:20rpx;
    position:relative;
}
.activeInfoImagesText{
    width:680rpx;
    min-height:60rpx;
    line-height:60rpx;
    font-size:28rpx;
    margin:0rpx auto;
}
.activeInfoImage{
    width:680rpx;
    min-height:100rpx;
    background:#fff;
    margin:0rpx auto 20rpx;
}
.activeInfoImage image{
    width:680rpx;
}
.emptyImage{
    width:720rpx;
    height:100rpx;
    background:transparent;
}
/* 活动介绍 活动规则 */
.activeInfoRule{
    width:720rpx;
    min-height:200rpx;
    background:#fff;
    margin-top:20rpx;
    position:relative;
    overflow:hidden;
}
.activeInfoRuleImage{
    width:680rpx;
    height:500rpx;
    background-color:lightgreen;
    margin:100rpx auto 20rpx;
}