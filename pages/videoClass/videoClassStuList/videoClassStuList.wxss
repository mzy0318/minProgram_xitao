@import '../../../templates/moreData/moreData.wxss';
@import '../../../templates/frozen/frozen.wxss';
page{
    padding-bottom:20rpx;
}
.videoClassList{
    width:710rpx;
    min-height:200rpx;
    margin:10rpx auto 0rpx;
}
.videoClassContent{
    width:345rpx;
    height:260rpx;
    background:#fff;
    border-radius:5px;
    vertical-align:top;
    display:inline-block;
    margin-top:15rpx;
}
.classContentImg{
    width:345rpx;
    height:200rpx;
}
.classContentImg image{
    width:345rpx;
    height:200rpx;
}
.classContentTitle{
    width:345rpx;
    height:60rpx;
    line-height:60rpx;
    font-size:24rpx;
    text-align:center;
    overflow: hidden;
    white-space: nowrap; 
    text-overflow:ellipsis;
}
view.videoClassContent:nth-child(even){
    margin-left:20rpx;
}