.lessonList{
    width:750rpx;
    min-height:1207rpx;
    background:#F2F2F2;
}
.lessonBanner{
    width:750rpx;
    height:350rpx;
}
.lessonBanner image{
    width:750rpx;
    height:350rpx;
}
.contentList{
    width:750rpx;
    height:200rpx;
    background:#fff;
    margin:0 auto 20rpx;
}
.contentListLeft{
    float:left;
    width:200rpx;
    height:150rpx;
    background:#fff;
    margin:25rpx 20rpx;
    overflow:hidden;
}
.contentListLeft image{
    width:200rpx;
    height:150rpx;
}
.contentListRight{
    width:480rpx;
    height:160rpx;
    float:left;
    margin-top:20rpx;
}
.contentListRightTop{
    font-size:30rpx;
    color:#101010;
}
.contentListRightMiddle{
    font-size:24rpx;
    color:#101010;
    margin-top:20rpx;
}
.contentListRightBottom{
    width:100%;
    line-height:60rpx;
    background:lightseagreen;
}
.contentListRightBottom>view:nth-child(1){
    float:left;

}
.contentListRightBottom>view:nth-child(1)>text:nth-child(1){
    font-size:24rpx;
    color:#7b7b7b;
    text-decoration:line-through;
}
.contentListRightBottom>view:nth-child(1)>text:nth-child(2){
    font-size:30rpx;
    color:#F86B3D;
}
.contentListRightBottom>view:nth-child(2){
    float:right;
    line-height:60rpx;
    width:130rpx;
    font-size:30rpx;
    color:#fff;
    background:#66ADFF;
    text-align:center;
    border-radius:5px;
}