    background:#fff;
    margin:0rpx auto;
    border-radius:10rpx;
    padding-top:1rpx;
    padding-bottom:10rpx;
}
.manageEditSixWrap{
    width:680rpx;
    height:380rpx;
    margin-left:15rpx;
