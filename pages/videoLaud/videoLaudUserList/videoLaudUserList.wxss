page{
    background:#F7F3F2;
}
.userListWrap{
    width:730rpx;
    height:220rpx;
    background:#fff;
    margin:20rpx auto 0rpx;
    border-radius:5px;
    box-shadow:1px 1px 5px 1px rgba(0,0,0,0.3);
}
.userListLeft{
    width:200rpx;
    height:180rpx;
    float:left;
    margin-left:20rpx;
    margin-top:20rpx;
    background:lightblue;
}
.userListLeft image{
    width:200rpx;
    height:180rpx;
}
.userListRight{
    width:480rpx;
    height:200rpx;
    float:left;
    margin-left:20rpx;
    margin-top:10rpx;
}
.userListRight>view{
    width:480rpx;
}
.userListRight>view:nth-child(1){
    line-height:50rpx;
    font-size:30rpx;
}
.userListRight>view:nth-child(2){
    margin-top:30rpx;
    line-height:30rpx;
    font-size:22rpx;
}
.userListRight>view:nth-child(3){
    height:60rpx;
    font-size:30rpx;
    color:#FF6634;
    line-height:80rpx;
    margin-top:25rpx;
}
.userListRight>view:nth-child(3)>button{
    width:130rpx;
    height:60rpx;
    background:#FF6634;
    color:#fff;
    font-size:24rpx;
    line-height:60rpx;
    float:right;
    margin-right:10rpx;
}