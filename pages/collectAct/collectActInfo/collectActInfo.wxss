@import '../../../utils/iconFont.wxss';
.infoBannerImage{
    width:750rpx;
    height:410rpx;
    background:#fff;
}
.infoBannerImage>image{
    width:750rpx;
    height:410rpx;
}
.infoBackImage{
    width:750rpx;
    min-height:100rpx;
    background:#fff;
    padding-bottom:130rpx;
    padding-top:30rpx;
}
.laudActImage,.laudActInfo,.actInfoConnect,.actInfoRange,.actInfoIntruc{
    width:690rpx;
    min-height:80rpx;
    background:#fff;
    margin:0rpx auto;
    border-radius:10px;
    padding-top:1px;
    padding-bottom:30rpx;
    box-shadow:.5px .5px 5px 2px rgba(192,192,192,.3);
}
.laudActImageTitle{
    width:640rpx;
    font-size:30rpx;
    font-weight:bold;
    border-left:5px solid #F8C001;
    margin:30rpx auto;
    line-height:40rpx;
}
.laudActImageTitle>text{
    width:620rpx;
    min-height:40rpx;
    display:block;
    margin-left:20rpx;
}
.laudActImages{
    width:640rpx;
    min-height:100rpx;
    margin:20rpx auto 0rpx;
    border-bottom:.5rpx solid #efefef;
}
.laudActImagesContent{
   width:128rpx;
   height:250rpx;
   display:inline-block;
   vertical-align:top;
   font-size:26rpx;
   text-align:center;
}
.laudActImagesContent>view>image{
    width:128rpx;
    height:140rpx;
}
.laudActImagesContent>view>text{
    display:block;
}
/* 个人信息 */
.laudActPersonInfo{
    width:640rpx;
    height:170rpx;
    border-bottom:.5rpx solid #efefef;
    margin:0rpx auto;
}
.personInfoImage{
    width:130rpx;
    height:130rpx;
    border-radius:50%;
    background:#fff;
    float:left;
    margin-top:20rpx;
}
.personInfoImage>image{
    width:130rpx;
    height:130rpx;
}
.personInfoText{
    width:410rpx;
    height:130rpx;
    float:left;
    margin-left:20rpx;
    margin-top:20rpx;
}
.personInfoText>view{
    font-size:26rpx;
    line-height:45rpx;
}
.personInfoText>view:nth-child(1)>text{
    font-size:35rpx;
}
/* 优惠 */
.collectActText,.collectText{
    width:640rpx;
    height:80rpx;
    margin:0rpx auto;
    font-size:30rpx;
    line-height:80rpx;
    color:#2E3035;
    text-align:center;
}
.collectActText{
    margin-top:30rpx;
}
.collectText>view{
    font-size:30rpx;
    line-height:40rpx;
    color:#2E3035;
    text-align:center;
}
.collectText>view>text{
    color:#FF77A5;
}
/* 参与者 */
.collectActRange{
    width:640rpx;
    height:80rpx;
    border-bottom:.5rpx solid #efefef;
    margin:0rpx auto;
}
.collectActRange>view{
    float:left;
}
.collectActRangeImage{
    width:50rpx;
    height:50rpx;
    border-radius:50%;
    background:#fff;
    margin-top:15rpx;
}.collectActRangeImage>image{
    width:50rpx;
    height:50rpx;
}
.collectActRangeName{
    width:100rpx;
    height:80rpx;
    line-height:80rpx;
    margin-left:10rpx;
    font-size:26rpx;
    color:#545454;
    overflow: hidden;
    white-space: nowrap; 
    text-overflow:ellipsis;
}
.collectActRangeTime{
    line-height:80rpx;
    color:#7b7b7b;
    margin-left:90rpx;
    font-size:22rpx;
}
.collectActRangeGetted{
    line-height:80rpx;
    font-size:26rpx;
    margin-left:10rpx;
}
.collectActRangeIcon{
    width:55rpx;
    height:55rpx;
    background:lightgreen;
    margin-left:8rpx;
    margin-top:12.5rpx;
}
/* 连接点 */
.viewMiddle{
    width:640rpx;
    height:24rpx;
    background:transparent;
    margin:0rpx auto;
}
.viewMiddle>view{
    width:7px;
    height:24rpx;
    background:#fff;
    float:left;
    margin-left:50rpx;
}
.viewMiddle>view:nth-child(2){
    float:right;
    margin-right:50rpx;
}
/* 优惠展示 */
.rewardInfoWrap{
    width:620rpx;
    margin:20rpx auto 0rpx;
    border:1rpx solid #efefef;
    border-radius:6px;
    padding:5rpx 0rpx;
}
.rewardInfoWrap>view{
    width:620rpx;
    line-height:40rpx;
    font-size:30rpx;
    text-align:center;
}
.rewardInfoWrap>view:nth-child(1)>text{
    color:#FF77A5;
}
.rewardInfoWrap>view:nth-child(1){
    font-size:26rpx;
}
/* 活动说明 */
.laudActInfoTitle{
    width:640rpx;
    height:50rpx;
    margin:20rpx auto 0rpx;
}
.laudActInfoTitle>view{
    width:140rpx;
    line-height:50rpx;
    font-size:30rpx;
    font-weight:bold;
    border-bottom:0.5px solid #FCCBAA;
    text-align:center;
}
.laudActInfoDate{
    width:640rpx;
    line-height:50rpx;
    font-size:26rpx;
    color:#212121;
    margin:15rpx auto 0rpx;
}
.laudActInfoWrap{
    width:620rpx;
    min-height:40rpx;
    margin:40rpx auto 0rpx;
}
.laudActInfoText{
    width:620rpx;
    min-height:35rpx;
    font-size:26rpx;
    color:#212121;
    background:#fff;
}
.laudActInfoImage{
    width:620rpx;
    min-height:0rpx;
}
.laudActInfoImage>image{
    width:620rpx;
}
/* 当前排名 */
.rangeTitle,.rangeContent{
    width:620rpx;
    height:80rpx;
    margin:0rpx auto;
}
.rangeTitle view,.rangeContent view{
    width:25%;
    height:80rpx;
    line-height:80rpx;
    float:left;
    font-size:26rpx;
    text-align:center;
    color:#7b7b7b;
    overflow: hidden;
    white-space: nowrap; 
    text-overflow:ellipsis;
}
.rangeContent{
    border-bottom:.5rpx solid #efefef;
}

/* 活动倒计时 */
.countDown{
    width:500rpx;
    line-height:50rpx;
    background:#FF77A5;
    color:#fff;
    font-size:26rpx;
    text-align:center;
    border-radius:0px 0px 8px 8px;
    position:fixed;
    top:0rpx;
    left:50%;
    margin-left:-250rpx;
}
/* 我要报名 */
.joinAct{
    width:250rpx;
    line-height:60rpx;
    background:#FF77A5;
    color:#fff;
    border:3px solid #FBCBA9;
    text-align:center;
    font-size:30rpx;
    border-radius:10px;
    position:fixed;
    bottom:50rpx;
    left:50%;
    margin-left:-125rpx;
}
/* 报名页面 */
.joinerWrap{
    width:100%;
    height:100%;
    position:fixed;
    left:0rpx;
    top:0rpx;
    background:rgba(0,0,0,0.3);
}
.joinerContent{
    width:670rpx;
    min-height:500rpx;
    background:#fff;
    border-radius:5px;
    position:absolute;
    left:50%;
    top:50%;
    margin-left:-335rpx;
    margin-top:-345rpx;
    padding-bottom:30rpx;
}
.joinerContent view{
    width:520rpx;
    height:80rpx;
    background:#F2F2F2;
    font-size:28rpx;
    margin:20rpx auto 0rpx;
    border-radius:3px;
    line-height:80rpx;
    text-align:center;
}
.joinerContent>view:nth-child(1){
    background:#fff;
}
.joinerContent view>input{
    width:500rpx;
    height:80rpx;
    margin-left:20rpx;
    font-size:28rpx;
    text-align:left;
}

.joinerContent button{
    width:520rpx;
    line-height:80rpx;
    margin-top:20rpx;
    background:#47BAFD;
    color:#fff;
    font-size:30rpx;
}
.closeDiv{
    width:40rpx !important;
    height:40rpx !important;
    border:.5rpx solid #efefef;
    position:absolute;
    right:-20rpx !important;
    top:-40rpx !important;
    border-radius:50% !important;
    font-size:35rpx !important;
    color:#7b7b7b;
    line-height:40rpx !important;
    text-align:center;
}
/* 更多 */
.moreData{
    width:200rpx;
    height:60rpx;
    font-size:28rpx;
    line-height:60rpx;
    color:#fff;
    background:#4993FE;
    margin-top:20rpx;
}
.moreDataed{
    width:200rpx;
    height:60rpx;
    font-size:28rpx;
    line-height:60rpx;
    color:#7b7b7b;
    background:#efefef;
    margin-top:20rpx;
}
/* 其他功能按钮 */
.otherOptionWrap{
    width:120rpx;
    height:120rpx;
    position:fixed;
    bottom:400rpx;
    right:50rpx;
}
.otherOptions{
    width:100rpx;
    height:100rpx;
    margin:10rpx auto;
    background:rgba(192,192,192,0.5);
    border-radius:50%;
    position:relative;
}
.otherOptions image{
    width:100rpx;
    height:100rpx;
}

.otherOptionsChild{
    width:120rpx;
    height:320rpx;
    background:transparent;
    position:absolute;
    top:-300rpx;
    right:0rpx;
}
.otherOptionsChild view{
    width:100%;
    line-height:60rpx;
    font-size:28rpx;
    text-align:center;
    background:rgba(109,160,212,0.8);
    margin-top:15rpx;
}
/* 其他功能模板 */
.switchBackgroundImage{
    width:750rpx;
    height:80rpx;
    background:#000;
    position:fixed;
    bottom:0rpx;
    left:0rpx;
}
.switchBackgroundImage view,.switchMusicOptions view{
    width:140rpx;
    line-height:50rpx;
    background:#797866;
    font-size:24rpx;
    text-align:center;
    float:left;
    margin-left:20rpx;
    color:#fff;
    margin-top:15rpx;
    border-radius:5px;
}

.switchBackgroundImage view:nth-child(2),.switchMusicOptions view:nth-child(2),.switchMusicOptions view:nth-child(3){
    background:#964499;
    float:right;
    margin-right:20rpx;
    color:#fff;
}
.switchMusic{
    width:750rpx;
    height:400rpx;
    position:fixed;
    bottom:0rpx;
    left:0rpx;
    background:#fff;
}
.switchMusicTitle{
    width:750rpx;
    height:70rpx;
    display:flex;
    justify-content: space-around;
    border-bottom:1rpx solid #efefef;
}

.switchMusicTitle view{
    font-size:28rpx;
    line-height:70rpx;
    text-align:center;
}
.switchMusicBorder{
    border-bottom:2rpx solid #964499;
    color:#964499;
}
.switchMusicContent{
    width:750rpx;
    min-height:250rpx;
    display:fles;
    justify-content: space-around;
}
.switchMusicContent view{
    width:30%;
    height:40rpx;
    float:left;
    margin-top:20rpx;
    margin-left:15rpx;
    font-size:24rpx;
    text-align:center;
}
.switchMusicOptions{
    width:750rpx;
    height:80rpx;
    background:#000;
}
.switchModelContent{
    width:750rpx;
    height:250rpx;
    background:#fff;
    overflow-x: scroll;
}

.switchModelContentChild{
    float:left;
    min-width:750rpx;
    height:200rpx;
    margin-top:25rpx;
    display:flex;
    justify-content: space-around;
    flex-direction:row;
}

.switchModelContentChild .modelContentChildContent{
    width:200rpx;
    height:150rpx;
    margin-left:20rpx;
    float:left;
    margin-top:25rpx;
    border:1rpx solid #fff;
 }
 .modelContentChildContent view:nth-child(1){
     width:200rpx;
     line-height:35rpx;
     font-size:26rpx;
     text-align:center;
     color:#000;
     overflow: hidden;
    white-space: nowrap; 
    text-overflow:ellipsis;
 } 
.modelContentChildContent view:nth-child(2){
     width:200rpx;
     height:115rpx;
     background:#fff;
 } 

 .modelContentChildContent view:nth-child(2) image{
     width:200rpx;
     height:115rpx;
 }
 /* 音乐控制 */
.musicControl{
    width:50rpx;
    height:50rpx;
    border-radius:50%;
    position:fixed;
    top:50rpx;
    right:30rpx;
    background:#964499;
}
.fontStyleT{
    display:block;
    color:#fff;
    float:left;
    margin-top:9rpx;
    margin-left:8rpx;
}

@keyframes headRotate{
  0% {transform: rotate(0deg);}
  100% {transform: rotate(360deg);}
}
view.viewRotate{
    animation: headRotate 5s linear;
    animation-iteration-count:infinite;
}