.courseClassWrap{
  width:750rpx;
  height:1207rpx;
  background:#F0EFED;
}
.empty{
  width:100%;
  height:20rpx;
  background:transparent;
}
.courseClassContent{
  width:720rpx;
  margin:0 auto;
}
.courseClassContent view{
  background:#fff;
  float:left;
  width:240rpx;
  line-height:80rpx;
  border-top:1px solid #7b7b7b;
  border-bottom:1px solid #7b7b7b;
  text-align:center;
  border-left:1px solid #7b7b7b;
  font-size:30rpx;
}
.courseClassBtn{
  clear:both;
  width:750rpx;
  height:60rpx;
  margin-top:1000rpx;
}
.courseClassBtn button{
  float:left;
  width:300rpx;
  line-height:60rpx;
  text-align:center;
  margin-left:50rpx;
}

.courseClassBtn button:nth-child(1){
  color:#7b7b7b;
}

