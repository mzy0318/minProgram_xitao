@import '../../../templates/frozen/frozen.wxss';
page{
    background:#E5E5E5;
}
.fractionContent{
    width:100%;
    height:100rpx;
    background:#fff;
    border-bottom:1px solid #efefef;
}
.fractionName{
    width:140rpx;
    line-height:100rpx;
    font-size:26rpx;
    color:#7b7b7b;
    text-align:right;
    float:left;
}
.fractionInput{
    width:300rpx;
    height:100rpx;
    float:left;
    margin-left:20rpx;
    line-height:100rpx;
    font-size:26rpx;
    color:#7b7b7b;
}
.fractionInput switch{
    margin-top:0rpx;
}
.fractionInput input{
    width:90rpx;
    height:60rpx;
    background:#E5E5E5;
    border-radius:5px;
    margin-top:20rpx;
    float:left;
    margin-right:15rpx;
    text-align:center;
    color:#000;
}
.fractionText{
    width:100%;
    height:550rpx;
    background:#fff;
}
.fractionTextarea{
    width:540rpx;
    height:510rpx;
    background:#E5E5E5;
    border-radius:5px;
    float:left;
    margin-top:20rpx;
    margin-left:20rpx;
}
.fractionTextarea textarea{
    width:520rpx;
    height:490rpx;
    margin-left:10rpx;
    margin-top:10rpx;
    font-size:30rpx;
}
.fractionBtn{
    width:600rpx;
    height:90rpx;
    line-height:90rpx;
    font-size:30rpx;
    margin-top:50rpx;
}