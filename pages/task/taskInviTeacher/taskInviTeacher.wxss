@import '../../../templates/frozen/frozen.wxss';
page{
    background:#fff;
}
/* 图片 */
.inviTeacher{
    width:400rpx;
    height:400rpx;
    margin:100rpx auto 0rpx;
}
.inviTeacher image{
    width:400rpx;
    height:400rpx;
}
/* 标题 */
.inviTeacherTitle{
    width:100%;
    height:100rpx;
    text-align:center;
    line-height:100rpx;
    font-size:35rpx;
}

/* 按钮 */
.inviTeacherBtn{
    width:100%;
    height:100rpx;
    margin-top:320rpx;
}
.inviTeacherBtn button{
    float:left;
    width:300rpx;
    height:100rpx;
    line-height:100rpx;
    font-size:35rpx;
    margin-left:50rpx;
}