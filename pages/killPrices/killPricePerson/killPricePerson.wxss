.pricePersonWrap{
    width:750rpx;
    min-height:1207rpx;
    background:#fff;
    background-size:100% 100%;
    background-repeat:repeat-y;
    padding-bottom:20rpx;
}
.killPriceBanner{
    width:750rpx;
    height:500rpx;
}
.killPriceBanner image{
    width:750rpx;
    height:500rpx;
}
/*第一部分  */
.pricePersonOne{
    width:670rpx;
    min-height:562rpx;
    margin:20rpx auto 0rpx;
    border-radius:5px 5px 0px 0px;
}
.pricePersonONeTitle{
    width:670rpx;
    min-height:80rpx;
    background:#fff;
    border-radius:5px 5px 0px 0px;
    border-bottom:1rpx solid #efefef;
    padding-bottom:10rpx;
}
.empty0{
    width:100%;
    height:25rpx;
    background:#fff !important;
    border-radius:5px 5px 0px 0px;
}
.pricePersonONeTitleText{
    width:600rpx;
    line-height:40rpx;
    min-height:40rpx;
    font-size:30rpx;
    font-weight:bold;
    margin:0rpx auto;
    border-left:3px solid #F8C002;
    margin-left:30rpx;
}
.pricePersonONeTitleText>text{
    display:block;
    width:580rpx;
    line-height:40rpx;
    min-height:40rpx;
    margin-left:20rpx;
}
.pricePersonONeRight{
    float:right;
    width:100rpx;
    line-height:35rpx;
    font-size:30rpx;
    font-weight:bold;
    text-align:right;
    margin-right:30rpx;
    color:#CC2200;
    margin-top:-37rpx;
}
.pricePersonOneRange{
    width:670rpx;
    min-height:410rpx;
    background:#fff;
    overflow:hidden;
    border-radius:0px 0px 5px 5px;
}
.pricePersonOneShow{
    width:670rpx;
    min-height:120rpx;
    position:relative;
}
.pricePersonOneShowChild{
    width:200rpx;
    line-height:60rpx;
    background:#CC2200;
    position:absolute;
    left:30rpx;
    top:30rpx;
    border-radius:5px;
    font-size:30rpx;
    text-align:center;
    color:#fff;
}
.pricePersonOneShowChild::before{
    position: absolute;
    display: block;
    width: 0;
    height: 0;
    border: solid transparent;
    pointer-events: none;
    content: "";
    border-color: rgba(136, 183, 213, 0);
}

.pricePersonOneRange .rangeWrap{
    width:600rpx;
    height:40rpx;
    background:#CACACA;
    margin:0rpx auto 0rpx;
    border-radius:30rpx;
}
.pricePersonOneRange .rangeWrap .rangeChild{
    width:100rpx;
    height:40rpx;
    background:#F4BD01;
    border-radius:30rpx;
}
.pricePersonOneRange .rangePrice{
    width:600rpx;
    height:200rpx;
    background:#fff;
    margin:0 auto;
}
.pricePersonOneRange .rangePrice .rangePriceLeft,.pricePersonOneRange .rangePrice .rangePriceRight{
    width:100rpx;
    height:100rpx;
    float:left;
    margin-left:20rpx;
    margin-top:20rpx;
}

.pricePersonOneRange .rangePrice .rangePriceRight{
    float:right;
    margin-right:20rpx;
}
.pricePersonOneRange .rangePriceLeft view,.pricePersonOneRange .rangePriceRight view{
    font-size:30rpx;
    text-align:center;
    margin-top:5rpx;
}

.pricePersonOneRange .rangePriceLeft view:nth-child(2),.pricePersonOneRange .rangePriceRight view:nth-child(2){
    color:#A8A8A8;
}
.pricePersonOneRange>button{
    width:210rpx;
    height:60rpx;
    background:#CC2200;
    color:#fff;
    line-height:60rpx;
    margin-top:-40rpx;
    font-size:30rpx;
    float:left;
    margin-left:80rpx;
}

.bottomLine view{
    width:15rpx;
    height:32rpx;
    background:#fff;
    float:left;
    margin-left:60rpx;
}

.bottomLine view:nth-child(2){
    float:right;
    margin-right:60rpx;
}
/* 去支付 */
.payMeny{
    width:670rpx;
    min-height:20rpx;
    background:#fff;
    margin:0rpx auto;
    padding-bottom:20rpx;
}
.payMeny>view{
    width:670rpx;
    height:80rpx;
    line-height:80rpx;
    text-align:center;
    font-size:30rpx;

}
.payMeny>view>text{
    color:#CC2200;
    font-size:36rpx;
    font-weight:bold;
}
.payMeny>button{
    width:260rpx;
    height:60rpx;
    line-height:60rpx;
    background:#CC2200;
    color:#fff;
    font-size:28rpx;
}
/*第二部分*/
.pricePersonTwo{
    width:670rpx;
    height:742rpx;
    margin:25rpx auto 0rpx;
    border-radius:5px 5px 0px 0px;
}
.pricePersonTwoImg{
    width:670rpx;
    height:630rpx;
    background:#fff;
    overflow:hidden;
}
.pricePersonTwoImgTop{
    width:400rpx;
    height:400rpx;
    background:#fff;
    margin:20rpx auto 0rpx;
}
.pricePersonTwoImgTop image{
    width:400rpx;
    height:400rpx;
}
.pricePersonTwoImgBottom{
    width:100%;
    line-height:40rpx;
    font-size:28rpx;
    text-align:center;
}
.pricePersonTwo .pricePersonTwoImg button{
    font-size:28rpx;
    line-height:60rpx;
    /* float:left; */
    color:#fff;
    background:#CC2200;
    margin-top:40rpx;
    width:300rpx;
    /* margin:40rpx 0rpx 0rpx 65rpx; */
}
/*第三部分  */
.pricePersonThree{
    width:670rpx;
    min-height:10rpx;
    margin:0 auto;
}
.pricePersonThreeText{
    width:100%;
    min-height:240rpx;
    background:#fff;
    padding-top:1rpx;
}
.pricePersonThreeText>view{
    font-size:28rpx;
    margin:30rpx 0rpx 0rpx 30rpx;
    color:#414141;
}
.pricePersonThreeImg{
    width:670rpx;
    min-height:550rpx;
    background:#fff;
}
.pricePersonThreeImgTitle{
    width:670rpx;
    line-height:40rpx;
    min-height:40rpx;
    font-size:28rpx;
    color:#7b7b7b;
    padding-top:1rpx;
}
.pricePersonThreeImgTitle>text{
    display:block;
    width:610rpx;
    margin:10rpx auto 0rpx;
}

.pricePersonThreeImgImage{
    width:610rpx;
    height:300rpx;
    background:lightgreen;
    margin:20rpx auto 0rpx;
    overflow:hidden;
}
.pricePersonThreeImgImage>image{
    width:610rpx;
    height:300rpx;
}
.pricePersonThreeImg button{
    font-size:28rpx;
    line-height:60rpx;
    float:left;
    color:#fff;
    background:#CC2200;
    margin:40rpx 0rpx 0rpx 90rpx;
}
/*第四部分  */
.pricePersonFour{
    width:670rpx;
    min-height:500rpx;
    background:#fff;
    margin:25rpx auto 0rpx;
}
.pricePersonFourContent{
    width:600rpx;
    min-height:340rpx;
    margin:25rpx auto 25rpx;
    background:#fff;
}
.pricePersonFourContent .contentTwo{
    width:100%;
    line-height:40rpx;
    font-size:30rpx;
    text-align:center;
    margin-top:50rpx;
}
.pricePersonFourContent .contentTwo text{
    color:#CC2200;
    font-size:35rpx;
}
.pricePersonFourContent .contentThree{
    width:100%;
    height:100rpx;
    margin-top:30rpx;
}

.pricePersonFourContent .contentThree view:nth-child(1){
    width:100rpx;
    height:100rpx;
    float:left;
    border-radius:50%;
    overflow: hidden;
}

.pricePersonFourContent .contentThree view:nth-child(1) image{
    width:100rpx;
    height:100rpx;
}
.pricePersonFourContent .contentThree view:nth-child(2){
    width:480rpx;
    height:100rpx;
}
.pricePersonFourContent .contentThree view:nth-child(2) text{
    display:block;
    line-height:100rpx;
    float:left;
    font-size:30rpx;
    margin-left:20rpx;
}
.pricePersonFourContent .contentThree view:nth-child(2) text:nth-child(2){
    text-align:right;
    float:right;
    margin-right:-80rpx;
    color:#CC2200;
}

