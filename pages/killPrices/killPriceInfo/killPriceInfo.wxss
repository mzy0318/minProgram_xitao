@import '../../../utils/iconFont.wxss';

.killPriceInfoWrap {
    width: 750rpx;
    min-height: 1207rpx;
    background-repeat: repeat;
    padding-bottom: 20rpx;
}

.killPriceBanner {
    width: 750rpx;
    height: 500rpx;
}

.killPriceBanner image {
    width: 750rpx;
    height: 500rpx;
}

/*第一部分  */

.killPriceInfoOne {
    width: 670rpx;
    min-height: 485rpx;
    border-radius: 5px 5px 0px 0px;
    margin: 20rpx auto 0rpx;
    background: transparent;
}

.killPriceInfoTop {
    width: 670rpx;
    min-height: 80rpx;
    background: #fff;
    border-radius: 5px 5px 0px 0px;
    border-bottom: 1rpx solid #efefef;
    box-shadow: 0px 2px 2px 2px #e2e6ed;
    padding-bottom: 10rpx;
}

.killPriceInfoTop>.killPriceInfoTopText {
    width: 600rpx;
    min-height: 40rpx;
    line-height: 40rpx;
    font-size: 30rpx;
    font-weight: bold;
    margin: 0rpx auto;
    border-left: 3px solid #f8c002;
}

.killPriceInfoTopText>text {
    display: block;
    width: 580rpx;
    margin-left: 20rpx;
    min-height: 40rpx;
}

.empty {
    width: 100%;
    height: 25rpx;
    background: #fff !important;
    border-radius: 5px 5px 0px 0px;
}

.killPriceInfoImg {
    width: 670rpx;
    height: 390rpx;
    background: #fff;
    overflow: hidden;
    box-shadow: 0px 2px 2px 2px #e2e6ed;
}

.killPriceInfoImg image {
    width: 600rpx;
    height: 350rpx;
    margin-top: 20rpx;
    margin-left: 35rpx;
    background: #fff;
}

.killPriceInfoRange {
    width: 670rpx;
    height: 400rpx;
    background: #fff;
    overflow: hidden;
    border-radius: 0px 0px 5px 5px;
    box-shadow: 0px 2px 2px 2px #e2e6ed;
}

.rangeWrap {
    width: 600rpx;
    height: 40rpx;
    background: #cacaca;
    margin: 20rpx auto 0rpx;
    border-radius: 30rpx;
    overflow:hidden;
}

.rangeWrap>.rangeChild{
    width: 600rpx;
    height: 40rpx;
    background: #f4bd01;
    border-radius: 30rpx;
}

.rangePrice {
    width: 600rpx;
    height: 200rpx;
    background: #fff;
    margin: 0 auto;
}

.rangePrice .rangePriceLeft, .rangePrice .rangePriceRight {
    width: 100rpx;
    height: 100rpx;
    float: left;
    margin-left: 20rpx;
    margin-top: 20rpx;
}

.rangePrice .rangePriceRight {
    float: right;
    margin-right: 20rpx;
}

.rangePriceLeft view, .rangePriceRight view {
    font-size: 30rpx;
    text-align: center;
    margin-top: 5rpx;
}

.rangePriceLeft view:nth-child(2), .rangePriceRight view:nth-child(2) {
    color: #a8a8a8;
}

.killPriceInfoRange button {
    width: 400rpx;
    background: #c20;
    color: #fff;
    line-height: 80rpx;
    margin-top: 15rpx;
    font-size: 35rpx;
}

.killPriceInfoBottom {
    width: 100%;
    height: 32rpx;
    background: transparent;
}

.killPriceInfoBottom view {
    width: 15rpx;
    height: 32rpx;
    background: #fff;
    float: left;
    margin-left: 60rpx;
}

.killPriceInfoBottom view:nth-child(2) {
    float: right;
    margin-right: 60rpx;
}

/*第二部分  */

.killPriceInfoTwo {
    width: 670rpx;
    min-height: 630rpx;
    margin: 0rpx auto;
    border-radius: 5rpx;
}

.killPriceInfoTwoTop {
    width: 670rpx;
    min-height: 960rpx;
    box-shadow: 0px 2px 2px 2px #e2e6ed;
    border-radius: 5rpx;
}

.infoTwoTitle {
    width: 670rpx;
    min-height: 80rpx;
    background: #fff;
    border-radius: 5px 5px 0px 0px;
    border-bottom: 1rpx solid #efefef;
}

.infoTwoTitleText {
    width: 600rpx;
    line-height: 35rpx;
    font-size: 30rpx;
    font-weight: bold;
    margin: 0rpx auto;
    text-indent: 20rpx;
    border-left: 3px solid #f8c002;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.infoTwoInfo {
    width: 100%;
    min-height: 310rpx;
    background: #fff;
    overflow: hidden;
    border-bottom: 1rpx solid #efefef;
}

.infoTwoInfo>view {
    font-size: 26rpx;
    margin: 30rpx 0rpx 0rpx 30rpx;
    color: #414141;
}

.infoTwoInfo>view>text {
    margin-left: 10rpx;
}

.infoTwoImg {
    width: 670rpx;
    min-height: 570rpx;
    background: #fff;
    padding-bottom:15rpx;
    overflow: hidden;
}

.infoTwoImgTitle {
    width: 670rpx;
    line-height: 40rpx;
    min-height: 40rpx;
    font-size: 28rpx;
    color: #7b7b7b;
}

.infoTwoImgTitle>text {
    display: block;
    width: 610rpx;
    margin: 10rpx auto 0rpx;
}

.infoTwoImgImage {
    width: 610rpx;
    margin: 0rpx auto;
}

.infoTwoImgImage image {
    width: 610rpx;
}

.infoTwoImg button {
    width: 300rpx;
    background: #c20;
    color: #fff;
    line-height: 80rpx;
    margin-top: 40rpx;
    font-size: 35rpx;
}

.killPriceInfoTwoBottom {
    width: 100%;
    height: 30rpx;
}

.killPriceInfoTwoBottom view {
    width: 15rpx;
    height: 32rpx;
    background: #fff;
    float: left;
    margin-left: 60rpx;
}
.killPriceInfoTwoBottom view:nth-child(2) {
    float: right;
    margin-right: 60rpx;
}

/*第三部分  */

.killPriceInfoThree {
    width: 670rpx;
    min-height: 600rpx;
    background: #fff;
    margin: -2rpx auto 0rpx;
    padding-bottom:15rpx;
    box-shadow: 0px 2px 2px 2px #e2e6ed;
}

.infoThreeTitle {
    width: 670rpx;
    height: 100rpx;
}

.infoThreeTitle view {
    width: 150rpx;
    line-height: 100rpx;
    float: left;
    text-align: center;
    font-size: 30rpx;
    font-weight: bold;
    margin-left: 20rpx;
}

.infoThreeTitle view:nth-child(1) {
    margin-left: -30rpx;
}

.borderStyle {
    border-bottom: 5rpx solid #f9c000;
}

.infoThreeContentOne {
    width: 600rpx;
    min-height: 450rpx;
    margin: 25rpx auto 0rpx;
}

.infoThreeContentOne .contentOne, .infoThreeContentOne .contentTwo {
    width: 100%;
    line-height: 40rpx;
    font-size: 30rpx;
    text-align: center;
    margin-top: 50rpx;
}

.infoThreeContentOne .contentOne text, .infoThreeContentOne .contentTwo text {
    color: #c20;
    font-size: 35rpx;
}

.infoThreeContentOne .contentThree {
    width: 100%;
    height: 100rpx;
    margin-top: 0rpx;
}

.contentThree>view:nth-child(1) {
    width: 80rpx;
    height: 80rpx;
    float: left;
    margin-top:10rpx;
    border-radius: 50%;
    background: #fff;
    overflow: hidden;
}

.contentThree>view:nth-child(1)>image {
    width: 80rpx;
    height: 80rpx;
}

.contentThree>view:nth-child(2) {
    width: 480rpx;
    height: 100rpx;
}

.contentThree>view:nth-child(2)>text{
    display: block;
    line-height: 100rpx;
    float: left;
    font-size: 30rpx;
    margin-left: 20rpx;
}
.contentThree>view:nth-child(2)>text:nth-child(1){
    width:120rpx;
    overflow: hidden;
    white-space: nowrap; 
    text-overflow:ellipsis;
}
.contentThree>view:nth-child(2)>text:nth-child(2) {
    width:240rpx;
    text-align: right;
    float: right;
    margin-right:-100rpx;
    color: #c20;
}

.infoThreeContentTwo {
    width: 600rpx;
    min-height: 450rpx;
    margin: 25rpx auto 25rpx;
}

.infoThreeContentTwo>text {
    display: block;
    font-size: 28rpx;
    color: #7b7b7b;
    margin-top: 20rpx;
    margin-left: 30rpx;
    line-height: 40rpx;
}

.infoThreeContentTwo text:nth-child(1) {
    margin-top: 60rpx;
}

.infoThreeContentTwo>button {
    width: 300rpx;
    background: #c20;
    color: #fff;
    line-height: 80rpx;
    margin-top: 40rpx;
    font-size: 35rpx;
}

/*弹窗  */

.writeInfoWrap {
    width: 750rpx;
    height: 1400rpx;
    position: fixed;
    top: 0rpx;
    left: 0rpx;
    background: rgba(0, 0, 0, 0.3);
}

.writeInfo {
    width: 660rpx;
    min-height: 400rpx;
    background: #fff;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -330rpx;
    margin-top: -300rpx;
    border-radius: 5px;
    padding-bottom: 20rpx;
}

.writeInfo>view:nth-child(1) {
    width: 690rpx;
    line-height: 40rpx;
    font-size: 30rpx;
    text-align: center;
    margin-top: 30rpx;
}

.writeInfo .inputStyle {
    width: 480rpx;
    height: 65rpx;
    line-height: 65rpx;
    background: #f2f2f2;
    margin: 40rpx auto 0rpx;
    border-radius: 5px;
}

.writeInfo .inputStyle input {
    width: 460rpx;
    height: 65rpx;
    line-height: 65rpx;
    background: #f2f2f2;
    font-size: 30rpx;
    border-radius: 5px;
    margin-left: 20rpx;
}

.writeInfo button {
    width: 480rpx;
    line-height: 60rpx;
    font-size: 30rpx;
    color: #fff;
    background: #47bafd;
    margin-top: 30rpx;
}

.line {
    width: 0rpx;
    height: 80rpx;
    border: 1rpx solid #fff;
    position: absolute;
    top: -80rpx;
    right: 60rpx;
}

.close {
    width: 50rpx;
    line-height: 50rpx;
    position: absolute;
    top: -130rpx;
    right: 35rpx;
    background: #fff;
    border-radius: 50%;
    text-align: center;
    font-size: 40rpx;
    color: #cacaca;
}

.otherOptionWrap {
    width: 120rpx;
    height: 120rpx;
    position: fixed;
    bottom: 400rpx;
    right: 50rpx;
}

.otherOptions {
    width: 100rpx;
    height: 100rpx;
    margin: 10rpx auto;
    background: rgba(192, 192, 192, 0.5);
    border-radius: 50%;
    position: relative;
}

.otherOptions image {
    width: 100rpx;
    height: 100rpx;
}

.otherOptionsChild {
    width: 120rpx;
    height: 320rpx;
    background: transparent;
    position: absolute;
    top: -300rpx;
    right: 0rpx;
}

.otherOptionsChild view {
    width: 100%;
    line-height: 60rpx;
    font-size: 28rpx;
    text-align: center;
    background: rgba(109,160,212,0.8);
    margin-top: 15rpx;
}

.iconStyle {
    width: 50rpx;
    height: 50rpx;
    font-size: 50rpx;
    display: block;
    color: #7b7b7b;
    top: 25rpx;
    left: 25rpx;
    position: absolute;
}

.switchBackgroundImage {
    width: 750rpx;
    height: 80rpx;
    background: #000;
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
}

.switchBackgroundImage view, .switchMusicOptions view {
    width: 140rpx;
    line-height: 50rpx;
    background: #797866;
    font-size: 24rpx;
    text-align: center;
    float: left;
    margin-left: 20rpx;
    color: #fff;
    margin-top: 15rpx;
    border-radius: 5px;
}

.switchBackgroundImage view:nth-child(2), .switchMusicOptions view:nth-child(2),
.switchMusicOptions view:nth-child(3) {
    background: #964499;
    float: right;
    margin-right: 20rpx;
    color: #fff;
}

.switchMusic {
    width: 750rpx;
    height: 400rpx;
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
    background: #fff;
}

.switchMusicTitle {
    width: 750rpx;
    height: 70rpx;
    display: flex;
    justify-content: space-around;
    border-bottom: 1rpx solid #efefef;
}

.switchMusicTitle view {
    font-size: 28rpx;
    line-height: 70rpx;
    text-align: center;
}

.switchMusicBorder {
    border-bottom: 2rpx solid #964499;
    color: #964499;
}

.switchMusicContent {
    width: 750rpx;
    min-height: 250rpx;
    display: fles;
    justify-content: space-around;
}

.switchMusicContent view {
    width: 30%;
    height: 40rpx;
    float: left;
    margin-top: 20rpx;
    margin-left: 15rpx;
    font-size: 24rpx;
    text-align: center;
}

.switchMusicOptions {
    width: 750rpx;
    height: 80rpx;
    background: #000;
}

.switchModelContent {
    width: 750rpx;
    height: 250rpx;
    background: #fff;
    overflow-x: scroll;
}

.switchModelContentChild {
    float: left;
    min-width: 750rpx;
    height: 200rpx;
    margin-top: 25rpx;
    display: flex;
    justify-content: space-around;
    flex-direction: row;
}

.switchModelContentChild .modelContentChildContent {
    width: 200rpx;
    height: 150rpx;
    margin-left: 20rpx;
    float: left;
    margin-top: 25rpx;
    border: 1rpx solid #fff;
}

.modelContentChildContent>view:nth-child(1) {
    width: 200rpx;
    line-height: 35rpx;
    font-size: 26rpx;
    text-align: center;
    color: #000;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.modelContentChildContent>view:nth-child(2) {
    width: 200rpx;
    height: 115rpx;
    background: #fff;
}

.modelContentChildContent view:nth-child(2) image {
    width: 200rpx;
    height: 115rpx;
}

/* 音乐控制 */

.musicControl {
    width: 50rpx;
    height: 50rpx;
    border-radius: 50%;
    position: fixed;
    top: 50rpx;
    right: 30rpx;
    background: #964499;
}

.fontStyleT {
    display: block;
    color: #fff;
    float: left;
    margin-top: 9rpx;
    margin-left: 8rpx;
}

@keyframes headRotate {
    0% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(360deg);
    }
}

view.viewRotate {
    animation: headRotate 5s linear;
    animation-iteration-count: infinite;
}
/* 更多数据 */
.moreData{
    width:600rpx;
    height:50rpx;
    line-height:50rpx;
    font-size:26rpx;
    color:#7b7b7b;
    text-align:center;
}
