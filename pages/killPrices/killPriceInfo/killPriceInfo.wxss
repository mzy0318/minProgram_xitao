@import '../../../utils/iconFont.wxss';
.killPriceInfoWrap{
    width:750rpx;
    min-height:1207rpx;
    background:#F9C000;
    background-size:100% 100%;
    background-repeat:repeat-y;
}

.killPriceBanner{
    width:750rpx;
    height:500rpx;
}
.killPriceBanner image{
    width:750rpx;
    height:500rpx;
}

/*第一部分  */

.killPriceInfoOne{
    width:670rpx;
    height:905rpx;
    border-radius:5px 5px 0px 0px;
    margin:0rpx auto 0rpx;
}

.killPriceInfoTop{
    width:670rpx;
    height:80rpx;
    background:#fff;
    border-radius:5px 5px 0px 0px;
    border-bottom:1rpx solid #efefef;
    box-shadow:0px 2px 2px 2px #E2E6ED;
}

.killPriceInfoTop .killPriceInfoTopText{
    width:600rpx;
    line-height:35rpx;
    font-size:30rpx;
    font-weight:bold;
    margin:0rpx auto;
    text-indent:20rpx;
    border-left:3px solid #F8C002;
    
}

.empty{
    width:100%;
    height:25rpx;
    background:#fff !important;
    border-radius:5px 5px 0px 0px;
}

.killPriceInfoImg{
    width:670rpx;
    height:390rpx;
    background:#fff;
    overflow:hidden;
    box-shadow:0px 2px 2px 2px #E2E6ED;
}
.killPriceInfoImg image{
    width:600rpx;
    height:350rpx;
    margin-top:20rpx;
    margin-left:35rpx;
    background:#0f0;
}

.killPriceInfoRange{
    width:670rpx;
    height:400rpx;
    background:#fff;
    overflow:hidden;
    border-radius:0px 0px 5px 5px;
    box-shadow:0px 2px 2px 2px #E2E6ED;
}

.rangeWrap{
    width:600rpx;
    height:40rpx;
    background:#CACACA;
    margin:20rpx auto 0rpx;
    border-radius:30rpx;
}

.rangeWrap .rangeChild{
    width:100rpx;
    height:40rpx;
    background:#F4BD01;
    border-radius:30rpx;
}
.rangePrice{
    width:600rpx;
    height:200rpx;
    background:#fff;
    margin:0 auto;
}

.rangePrice .rangePriceLeft,.rangePrice .rangePriceRight{
    width:100rpx;
    height:100rpx;
    float:left;
    margin-left:20rpx;
    margin-top:20rpx;
}

.rangePrice .rangePriceRight{
    float:right;
    margin-right:20rpx;
}

.rangePriceLeft view,.rangePriceRight view{
    font-size:30rpx;
    text-align:center;
    margin-top:5rpx;
}

.rangePriceLeft view:nth-child(2),.rangePriceRight view:nth-child(2){
    color:#A8A8A8;
}


.killPriceInfoRange button{
    width:400rpx;
    background:#CC2200;
    color:#fff;
    line-height:80rpx;
    margin-top:15rpx;
    font-size:35rpx;
}

.killPriceInfoBottom{
    width:100%;
    height:32rpx;
}

.killPriceInfoBottom view{
    width:15rpx;
    height:32rpx;
    background:#fff;
    float:left;
    margin-left:60rpx;
}

.killPriceInfoBottom view:nth-child(2){
    float:right;
    margin-right:60rpx;
}



/*第二部分  */
.killPriceInfoTwo{
    width:670rpx;
    min-height:990rpx;
    margin:-2rpx auto;
    border-radius:5rpx;
}

.killPriceInfoTwoTop{
    width:670rpx;
    height:960rpx;
    box-shadow:0px 2px 2px 2px #E2E6ED;
    border-radius:5rpx;
}

.infoTwoTitle{
   width:670rpx;
    height:80rpx;
    background:#fff;
    border-radius:5px 5px 0px 0px;
    border-bottom:1rpx solid #efefef;
}

.infoTwoTitleText{
    width:600rpx;
    line-height:35rpx;
    font-size:30rpx;
    font-weight:bold;
    margin:0rpx auto;
    text-indent:20rpx;
    border-left:3px solid #F8C002;
}

.infoTwoInfo{
    width:100%;
    height:310rpx;
    background:#fff;
    overflow:hidden;
    border-bottom:1rpx solid #efefef;
}

.infoTwoInfo view{
    font-size:28rpx;
    margin:30rpx 0rpx 0rpx 30rpx;
    color:#414141;
}

.infoTwoInfo view text{
    margin-left:40rpx;
}


.infoTwoImg{
    width:670rpx;
    min-height:570rpx;
    background:#fff;
    overflow:hidden;
}


.infoTwoImgTitle{
    width:670rpx;
    line-height:80rpx;
    font-size:30rpx;
    text-indent:30rpx;
}

.infoTwoImgImage{
    width:610rpx;
    height:300rpx;
    margin:0 auto;
}
.infoTwoImgImage image{
    width:610rpx;
    height:300rpx;
 }

.infoTwoImg button{
    width:300rpx;
    background:#CC2200;
    color:#fff;
    line-height:80rpx;
    margin-top:40rpx;
    font-size:35rpx;
}

.killPriceInfoTwoBottom{
    width:100%;
    height:30rpx;
}

.killPriceInfoTwoBottom view{
    width:15rpx;
    height:32rpx;
    background:#fff;
    float:left;
    margin-left:60rpx;
}

.killPriceInfoTwoBottom view:nth-child(2){
    float:right;
    margin-right:60rpx;
}


/*第三部分  */
.killPriceInfoThree{
    width:670rpx;
    min-height:600rpx;
    background:#fff;
    margin:-2rpx auto 0rpx;
    box-shadow:0px 2px 2px 2px #E2E6ED;
}

.infoThreeTitle{
    width:670rpx;
    height:100rpx;
}

.infoThreeTitle view{
    width:150rpx;
    line-height:100rpx;
    float:left;
    text-align:center;
    font-size:30rpx;
    font-weight:bold;
    margin-left:20rpx;
}

.infoThreeTitle view:nth-child(1){
    margin-left:-30rpx;
}
.borderStyle{
    border-bottom:5rpx solid #F9C000;
}
.infoThreeContentOne{
    width:600rpx;
    min-height:450rpx;
    margin:25rpx auto 0rpx;
}
.infoThreeContentOne .contentOne,.infoThreeContentOne .contentTwo{
    width:100%;
    line-height:40rpx;
    font-size:30rpx;
    text-align:center;
    margin-top:50rpx;
}
.infoThreeContentOne .contentOne text,.infoThreeContentOne .contentTwo text{
    color:#CC2200;
    font-size:35rpx;
}
.infoThreeContentOne .contentThree{
    width:100%;
    height:100rpx;
    margin-top:30rpx;
}

.contentThree view:nth-child(1){
    width:100rpx;
    height:100rpx;
    float:left;
    border-radius:50%;
    background:lightblue;
    overflow:hidden;
}
.contentThree view:nth-child(1) image{
    width:100rpx;
    height:100rpx;
}
.contentThree view:nth-child(2){
    width:480rpx;
    height:100rpx;
}
.contentThree view:nth-child(2) text{
    display:block;
    line-height:100rpx;
    float:left;
    font-size:30rpx;
    margin-left:20rpx;
}
.contentThree view:nth-child(2) text:nth-child(2){
    text-align:right;
    float:right;
    margin-right:-80rpx;
    color:#CC2200;
}
.infoThreeContentTwo{
    width:600rpx;
    height:450rpx;
    margin:25rpx auto 25rpx;
}

.infoThreeContentTwo text{
    display:block;
    font-size:30rpx;
    margin-top:20rpx;
    margin-left:30rpx;
    line-height:40rpx;
}
.infoThreeContentTwo text:nth-child(1){
    
    margin-top:60rpx;
}
.infoThreeContentTwo button{
    width:300rpx;
    background:#CC2200;
    color:#fff;
    line-height:80rpx;
    margin-top:40rpx;
    font-size:35rpx;
}

/*弹窗  */
.writeInfoWrap{
    width:750rpx;
    height:1207rpx;
    position:fixed;
    top:0rpx;
    left:0rpx;
    background:rgba(0,0,0,0.4);
}
.writeInfo{
    width:660rpx;
    height:400rpx;
    background:#fff;
    position:absolute;
    top:50%;
    left:50%;
    margin-left:-330rpx;
    margin-top:-200rpx;
    border-radius:5px;
}

.writeInfo view:nth-child(1){
    width:690rpx;
    line-height:40rpx;
    font-size:30rpx;
    text-align:center;
    margin-top:30rpx;
}
.writeInfo view:nth-child(2),.writeInfo view:nth-child(3){
    width:480rpx;
    line-height:80rpx;
    background:#F2F2F2;
    margin:40rpx auto 0rpx;
    border-radius:5px;
}
.writeInfo view:nth-child(2) input,.writeInfo view:nth-child(3) input{
    width:460rpx;
    line-height:80rpx;
    background:#F2F2F2;
    font-size:30rpx;
    border-radius:5px;
    margin-left:20rpx;
}
.writeInfo button{
    width:480rpx;
    line-height:60rpx;
    font-size:30rpx;
    color:#fff;
    background:#47BAFD;
    margin-top:30rpx;
}
.line{
    width:0rpx;
    height:80rpx;
    border:1rpx solid #fff;
    position:absolute;
    top:-80rpx;
    right:60rpx;
}
.close{
    width:50rpx;
    line-height:50rpx;
    position:absolute;
    top:-130rpx;
    right:35rpx;
    background:#fff;
    border-radius:50%;
    text-align:center;
    font-size:40rpx;
    color:#CACACA;
}

.otherOptionWrap{
    width:120rpx;
    height:120rpx;
    position:fixed;
    bottom:400rpx;
    right:50rpx;
}

.otherOptions{
    width:100rpx;
    height:100rpx;
    margin:10rpx auto;
    background:rgba(192,192,192,0.5);
    border-radius:50%;
    position:relative;
}

.otherOptionsChild{
    width:120rpx;
    height:450rpx;
    background:transparent;
    position:absolute;
    top:-400rpx;
    right:0rpx;
}
.otherOptionsChild view{
    width:100%;
    line-height:60rpx;
    font-size:28rpx;
    text-align:center;
     background:rgba(192,192,192,0.8);
    margin-top:15rpx;
}
.iconStyle{
    width:50rpx;
    height:50rpx;
    font-size:50rpx;
    display:block;
    color:#7b7b7b;
    top:25rpx;
    left:25rpx;
    position:absolute;  
}
.switchBackgroundImage{
    width:750rpx;
    height:80rpx;
    background:#000;
    position:fixed;
    bottom:0rpx;
    left:0rpx;
}

.switchBackgroundImage view,.switchMusicOptions view{
    width:140rpx;
    line-height:50rpx;
    background:#797866;
    font-size:24rpx;
    text-align:center;
    float:left;
    margin-left:20rpx;
    color:#fff;
    margin-top:15rpx;
    border-radius:5px;
}

.switchBackgroundImage view:nth-child(2),.switchMusicOptions view:nth-child(2),.switchMusicOptions view:nth-child(3){
    background:#964499;
    float:right;
    margin-right:20rpx;
    color:#fff;
}

.switchMusic{
    width:750rpx;
    height:400rpx;
    position:fixed;
    bottom:0rpx;
    left:0rpx;
    background:#fff;
}
.switchMusicTitle{
    width:750rpx;
    height:70rpx;
    display:flex;
    justify-content: space-around;
    border-bottom:1rpx solid #efefef;
}

.switchMusicTitle view{
    font-size:28rpx;
    line-height:70rpx;
    text-align:center;
}
.switchMusicBorder{
    border-bottom:2rpx solid #964499;
    color:#964499;
}
.switchMusicContent{
    width:750rpx;
    min-height:250rpx;
    display:fles;
    justify-content: space-around;
}
.switchMusicContent view{
    width:30%;
    height:40rpx;
    float:left;
    margin-top:20rpx;
    margin-left:15rpx;
    font-size:24rpx;
    text-align:center;
}
.switchMusicOptions{
    width:750rpx;
    height:80rpx;
    background:#000;
}

.switchModelContent{
    width:750rpx;
    height:250rpx;
    background:#fff;
    overflow-x: scroll;
}

.switchModelContentChild{
    float:left;
    min-width:750rpx;
    height:200rpx;
    margin-top:25rpx;
    display:flex;
    justify-content: space-around;
    flex-direction:row;
}

.switchModelContentChild .modelContentChildContent{
    width:200rpx;
    height:150rpx;
    margin-left:20rpx;
    float:left;
    margin-top:25rpx;
 }
 .modelContentChildContent view:nth-child(1){
     width:200rpx;
     line-height:35rpx;
     font-size:26rpx;
     text-align:center;
     color:#000;
 } 
.modelContentChildContent view:nth-child(2){
     width:200rpx;
     height:115rpx;
     background:red;
 } 

 .modelContentChildContent view:nth-child(2) image{
     width:200rpx;
     height:115rpx;
 }


