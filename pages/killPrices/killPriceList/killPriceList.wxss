@import '../../../utils/iconFont.wxss'; 

.killPriceListWrap{
    width:750rpx;
    min-height:1207rpx;
    background:#fff;
}
.killPriceListContet{
    width:710rpx;
    height:120rpx;
    background:#fff;
    margin:0 auto 20rpx;
    box-shadow: 3px 3px 3px 3px rgba(0,0,0,0.3);
}
.killPriceListTop,.killPriceListBottom{
    width:100%;
    height:60rpx;
}
.killPriceListTop text:nth-child(2){
    float:right;
    margin-right:20rpx;
    font-size:30rpx;
    color:#efefef;
    line-height:60rpx;
}
.killPriceListTop text:nth-child(1),.killPriceListBottom text:nth-child(1){
    float:left;
    margin-left:20rpx;
}
.iconColor{
    color:#5CBBF2;
    font-size:30rpx;
}

.killPriceListBottom text:nth-child(2){
    font-size:30rpx;
    line-height:60rpx;
    float:right;
    font-weight:bold;
    margin-right:25rpx;
    color:rgba(0,0,0,0.6);
}

.collageListWrap{
    width:750rpx;
    height:1207rpx;
    position:fixed;
    top:0rpx;
    left:0rpx;
    z-index:1000;
    background:#F5F5F5;
}
.collagePersonListTop{
    width:690rpx;
    height:325rpx;
    background:#fff;
    margin:0 auto;
}
.childView{
    width:100%;
    line-height:80rpx;
    border-bottom:1rpx solid #efefef;
    font-size:30rpx;
    text-indent:20rpx;
    color:#7b7b7b;
}
.collagePersonListMiddle{
    width:690rpx;
    height:485rpx;
    background:#fff;
    margin:40rpx auto;
}
.collagePersonListBottom{
    width:690rpx;
    height:165rpx;
    background:#fff;
    margin:0rpx auto;
}
.collageListWrap>button{
    width:400rpx;
    line-height:80rpx;
    margin-top:50rpx;
}