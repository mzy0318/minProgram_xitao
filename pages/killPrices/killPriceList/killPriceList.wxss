@import '../../../utils/iconFont.wxss'; 

page{
    height:100%;
    background:#fff;
}
.killPriceListWrap{
    width:750rpx;
    min-height:1207rpx;
    background:#fff;
    padding-bottom:20rpx;
}
.killPriceListContet{
    padding-top:20rpx;
    width:710rpx;
    height:120rpx;
    background:#fff;
    margin:0 auto 20rpx;
    box-shadow: 1rpx 1rpx 1rpx 1rpx rgba(0,0,0,0.1);
    border:1px solid #efefef;
    border-radius: 5rpx;
}
.killPriceListTop,.killPriceListBottom{
    width:100%;
    height:60rpx;
}
.killPriceListTop>text:nth-child(2){
    float:right;
    margin-right:20rpx;
    font-size:30rpx;
    color:#7b7b7b;
    line-height:60rpx;
}
.killPriceListTop>text:nth-child(1),.killPriceListBottom text:nth-child(1){
    float:left;
    margin-left:20rpx;
}
.killPriceListTop>text:nth-child(1){
    overflow: hidden;
    white-space: nowrap; 
    text-overflow:ellipsis;
}
.iconColor{
    color:#5CBBF2;
    font-size:30rpx;
}

.killPriceListBottom text:nth-child(2){
    font-size:30rpx;
    line-height:60rpx;
    float:right;
    font-weight:bold;
    margin-right:25rpx;
    color:rgba(0,0,0,0.6);
}

.collageListWrap{
    width:100%;
    height:100%;
    position:fixed;
    top:0rpx;
    left:0rpx;
    z-index:1000;
    background:#F5F5F5;
}
.collagePersonListTop{
    width:690rpx;
    min-height:85rpx;
    background:#fff;
    margin:0 auto;
}
.childView{
    width:100%;
    line-height:80rpx;
    border-bottom:1rpx solid #efefef;
    font-size:30rpx;
    text-indent:20rpx;
    color:#7b7b7b;
}
.collagePersonListMiddle{
    width:690rpx;
    min-height:85rpx;
    background:#fff;
    margin:40rpx auto;
}
.collagePersonListBottom{
    width:690rpx;
    height:165rpx;
    background:#fff;
    margin:0rpx auto;
}
.collageListWrap>button{
    width:400rpx;
    line-height:80rpx;
    margin-top:50rpx;
}
.empty{
    width:750rpx;
    height:20rpx;
    background:transparent;
}
.price{
    color:#C41A16;
}

/* 更多 */
.moreData{
    width:200rpx;
    height:60rpx;
    font-size:28rpx;
    line-height:60rpx;
    color:#fff;
    background:#4993FE;
}
.moreDataed{
    width:200rpx;
    height:60rpx;
    font-size:28rpx;
    line-height:60rpx;
    color:#7b7b7b;
    background:#efefef;
}